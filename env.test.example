# Test Environment Configuration
# Copy to .env.test and update values

# Local Test API (different port from prod)
DJANGO_TEST_API_URL=http://localhost:8001
DJANGO_TEST_JWT_TOKEN=your-test-jwt-token

# Staging Environment  
DJANGO_STAGING_API_URL=https://staging-api.yourcompany.com
DJANGO_STAGING_JWT_TOKEN=your-staging-jwt-token

# External Test API
EXTERNAL_TEST_API_URL=https://test-external-service.com/api
EXTERNAL_TEST_API_KEY=your-test-external-api-key

# Azure Test Storage
AZURE_TEST_CONNECTION_STRING=your-test-azure-connection-string
AZURE_TEST_CONTAINER_NAME=test-container 